{
    "schemaVersion": 1,
    "id": "plasmo_voice",
    "version": "${version}",
    "name": "Plasmo Voice",
    "description": "A proximity voice chat mod with audio positioning and lots of features",
    "authors": [
        "Plasmo"
    ],
    "contact": {
        "homepage": "https://github.com/plasmoapp/plasmo-voice",
        "sources": "https://github.com/plasmoapp/plasmo-voice",
        "issues": "https://github.com/plasmoapp/plasmo-voice/issues"
    },
    "license": "All rights reserved",
    "icon": "assets/plasmo_voice/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "su.plo.voice.client.VoiceClientFabric"
        ],
        "main": [
            "su.plo.voice.server.VoiceServerFabric"
        ]
    },
    "mixins": [
        "plasmovoice-common.mixins.json",
        "plasmovoice.mixins.json"
    ],
    "depends": {
        "fabric": ">=${fabric_min_version}",
        "fabricloader": ">=${loader_version}"
    },
    "suggests": {
        "soundphysics": "*"
    }
}
